end

to-report ISP-coverage  ;; reports % of internet users that are purchasing access
  let temp 0
  ask IUs [
    if contracted-ISP = "none" [ set temp ( temp + 1 ) ]
  ]
  report ( ( ( count IUs ) - temp ) / ( count IUs ) ) * 100
end

to-report ISP-infrastructure-quality
  report mean [  infrastructure-quality ] of ISPs
end

to-report ISP-infrastructure-size
  report mean [ infrastructure-size ] of ISPs
end

to-report AP-population
  report count APs
end

to-report AP-average-quality
  report mean [ portfolio-quality ] of  APs
end

to-report AP-average-variety
  report mean [ portfolio-variety ] of  APs
end

to-report min-ICT-tech
  report [ bt-quality ] of min-one-of ICTs [ bt-quality ]
end

to-report max-ICT-tech
  report [ bt-quality ] of max-one-of ICTs [ bt-quality ]
end

to-report mean-ICT-tech
