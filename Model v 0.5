end

to-report ICT-tech-cost
  report mean [ bt-cost ] of ICTs * mean [ bt-quality ] of ICTs
end


;;
;; Internet user procedures
;;

to-report IU-access-utility [ desired-ISP desired-IU ]  ;;     ;; return IU utility from ISP
  let utility 0
  let congestion-tolerance [ IU-congestion-tolerance ] of desired-IU
  let internet-usage [ IU-internet-usage ] of desired-IU
  let user-profile [ IU-profile ] of desired-IU
  let budget ( [ IU-budget ] of desired-IU ) * ( 0.85 + ( regulator-policy-stimulate-demand * 0.05 ) )
  let min-tech [ IU-mintech-req ] of desired-IU
  ifelse desired-ISP = "none" [ set utility 0 ]
  [
    let asking-price ( [ ISP-access-price ] of desired-ISP ) + ( [ ISP-utilization-price ] of desired-ISP ) * ( internet-usage * ( 0.7 - ( regulator-policy-stimulate-demand * 0.1 ) ) )
    let congestion [ ISP-congestion ] of desired-ISP
    ask desired-ISP [ set budget budget * ( ( ( [ infrastructure-quality ] of desired-ISP - 1 ) / ( 8 - ( 2 * user-profile ) ) ) + 1 ) ]

    ifelse budget >= asking-price [
      ask desired-ISP [
        ifelse congestion-tolerance >= congestion or min-tech <=  infrastructure-quality [
          ifelse congestion-tolerance >= congestion and min-tech <= infrastructure-quality
          [ set utility 1 ] [ set utility random-normal ( 0.2 + ( ( user-profile - 1) / 10 ) ) 0.5 ]
        ]
        [
          ifelse congestion-tolerance < congestion and min-tech > infrastructure-quality
          [ set utility -1 ] [ set utility random-normal ( 0 - ( ( user-profile - 1) / 10 ) ) 0.5 ]
        ]
      ]
    ]
    [ set utility -1 ]
  ]

  report utility
end


to IU-procedure          ;; ask IUs to defines from which ISP to purchase Internet access
  set-profile

  if IU-access-utility contracted-ISP self <= 0 [
    set patience patience + 1
    chose-ISP
    ifelse IU-access-utility contracted-ISP self < 0 [ ;; If the IU gets negative utility for more than 3 turns, he runs out of patience and does not purchase access
      if  patience > 3 [ set contracted-ISP "none" ]
    ]
    [ set patience 0 ]
  ]

  ;;let old-AP prefered-AP
  chose-AP

  ask my-links [ die ]

  ;;ifelse prefered-AP = old-AP [ create-link-with prefered-AP ] [ ask my-links with [ member? other-end APs ] [ die ] ]
  create-link-with prefered-AP
  ask my-links with [member? other-end APs] [ set color red ]

  ifelse contracted-ISP != "none" [ create-link-with contracted-ISP ] [ ask my-links with [ member? other-end ISPs ] [ die ] ]
  ask my-links with [member? other-end ISPs] [ set color green ]
end

to set-profile
  if IU-profile = 1 [
    set IU-mintech-req  min-infrastructure-quality ]
  if IU-profile = 2 [ set IU-mintech-req  ISP-infrastructure-quality ]
  if IU-profile = 3 [ set IU-mintech-req ISP-infrastructure-quality * 1.1 ]
end

to chose-ISP ;; chose another ISP to contract access ( has a chance to stick with current ISP, but the chance is smaller the better the rivals offers)
  let new-ISP contracted-ISP
  let desired-IU self
  ask ISPs [
    if IU-access-utility self desired-IU > 0 [
      if random-in-range 0 2 = 0 [ set new-ISP self ]
    ]
  ]
  set contracted-ISP new-ISP
end

to chose-AP ;; IU have higher chance to use APs with larger users base (network effect) and/or with better quality
  let new-AP prefered-AP
  let min-tech IU-mintech-req
  ask APs [
    if AP-average-quality > portfolio-quality [
      if random-in-range 0 100 < 50 [
        ;;if ( member? desired-segment segment = true ) and ( portfolio-quality > AP-average-quality desired-segment ) [
          set new-AP self
          set min-tech portfolio-mintech-req
        ;;]
      ]
    ]
  ]

  set prefered-AP new-AP
  set IU-mintech-req min-tech
end

;;
;; Internet Service Provider procedures
;;

to calculate-ISP-congestion  ;; return ISP congestion (total consumers * usage level ) / infrastructure-size
  let temp 0
  let me self
  ask IUs [
    if contracted-ISP = me [ set temp temp + ( ( IU-internet-usage * ( ( ( IU-profile - 1 ) / 10 ) + 1 ) * [ infrastructure-quality ] of contracted-ISP ) * ( 0.7 + ( regulator-policy-stimulate-demand * 0.1 ) ) ) ]
  ]
  set ISP-congestion ( ( temp / infrastructure-size ) * ( 1 + ( regulator-policy-coinvestment * 0.02 ) ) )
end

to-report ISP-average-investment-growth
  report ISP-total-investment-growth / ticks
end

to-report min-infrastructure-quality
  report [ infrastructure-quality ] of min-one-of ISPs [ infrastructure-quality ]
end

to-report max-infrastructure-quality
  report [ infrastructure-quality ] of max-one-of ISPs [ infrastructure-quality ]
end

to-report min-infrastructure-size
  report [ infrastructure-size ] of min-one-of ISPs [ infrastructure-size ]
end

to-report max-infrastructure-size
  report [ infrastructure-size ] of max-one-of ISPs [ infrastructure-size ]
end

to-report min-ISP-access-price
  report [ ISP-access-price ] of min-one-of ISPs [ ISP-access-price ]
end

to-report max-ISP-access-price
  report [ ISP-access-price ] of max-one-of ISPs [ ISP-access-price ]
end

to-report min-ISP-utilization-price
  report [ ISP-utilization-price ] of min-one-of ISPs [ ISP-utilization-price ]
end

to-report max-ISP-utilization-price
  report [ ISP-utilization-price ] of max-one-of ISPs [ ISP-utilization-price ]
end

to ISP-investment     ;; defines ISP investment (maybe inside ISP-procedure)
  ifelse infrastructure-quality < best-ICT-tech-available * ( 0.3 + ( regulator-policy-min-quality * 0.06 ) ) [ ISP-new-tech-investment ]  ;; ISPs nunca tem tecnologia 30% inferior a pior do mercado, e regulador exige no máximo 80% inferior ao melhor
  [
    if ISP-strategic-behavior = "expand" [ ISP-expand-investment ]
    if ISP-strategic-behavior = "new tech" [ ISP-new-tech-investment ]
    if ISP-strategic-behavior = "decrease price" [ ISP-define-price ]
  ]
end

to ISP-define-strategic-behavior
  ifelse ISP-good-sales? = false [
    ifelse patience > 15 [ set patience patience - 5 ] [ set patience patience - 1 ]
    ifelse ISP-can-decrease-prices? = true [ set ISP-strategic-behavior "decrease price" ] [ ISP-investment-strategy ]
  ]
  [
    set ISP-strategic-behavior "do nothing"
    set patience patience + 1
  ]

  if patience <= 0 [
    change-ISP-strategy
    set patience random-in-range 5 15
  ]
end

to ISP-investment-strategy
  ifelse ISP-can-make-investment? = true [
    ifelse ISP-is-network-congested? = true [ set ISP-strategic-behavior "expand" ] [ set ISP-strategic-behavior "new tech" ]
  ] [ set ISP-strategic-behavior "do nothing" ]
end

to-report ISP-good-sales? ;;; report true if number of clients is above minimum desired
  let temp 0
  let temp-ISP self
  ask IUs [
    if contracted-ISP = temp-ISP [ set temp temp + 1 ]
  ]
  ifelse temp >= ISP-target-sales [ report true ] [ report false ]
end

to-report ISP-can-decrease-prices?
  ifelse ISP-access-price > ISP-marginal-cost [ report true ] [report false ]
end

to-report ISP-can-make-investment?
  ifelse  ISP-return-investment-time <= 0 [ report true ] [ report false ]
end

to-report ISP-marginal-cost
  ifelse infrastructure-cost > 1 [ report ISP-mg-cost * ( 1 + ( ( infrastructure-cost - 1 ) / 2) ) ] [ report ISP-mg-cost ]
end

to-report ISP-is-network-congested?
  ifelse ISP-congestion > 1.3 - ( 0.1 * regulator-policy-min-congestion ) [ report true ]
  [ ifelse ISP-congestion > ISP-accepted-congestion [ report true ] [ report false ] ]
end

to ISP-expand-investment  ;; expands infrastructure size and increase marginal costs
  if random-in-range 0 100 < ( ( 10 * regulator-policy-coinvestment ) + 30 ) [
    set infrastructure-size infrastructure-size * ( 1 + ( infrastructure-quality / 20 ) )
    set ISP-return-investment-time 15 * ( 1.75 - ( regulator-policy-incentivize-ISP * 0.25 ) )
  ]
end

to change-ISP-strategy
  if ISP-strategy = "increase quality" [
    set ISP-strategy "decrease cost"
    set ISP-target-sales random-normal ( IU-initial-population * 0.9 / ( ISP-initial-population ) ) ( 0.5 * ( IU-initial-population / ISP-initial-population ) )
    set ISP-markup 1
  ]
  if ISP-strategy = "decrease cost" [
    set ISP-strategy "increase quality"
    set ISP-target-sales random-normal ( IU-initial-population * 0.9 / ( ISP-initial-population ) ) ( 0.5 * ( IU-initial-population / ISP-initial-population ) )
    set ISP-markup random-normal 1.12 0.015
  ]
end

to ISP-new-tech-investment ;; implement new technology, keep infrastructure size and (may) increase marginal (ISP is loyal to original ICT, but has a chance of changing ICT depending on their propensity to risk)
  if ISP-strategy = "increase quality" [ ;; tem 30% de escolher a melhor ICT se a qualidade dessa ICT for melhor que a atual, compra tecnologia dela
    ifelse random-in-range 0 100 < 30 [ set contracted-ICT who-best-infrastructure-quality ] [ set contracted-ICT one-of ICTs ]

    if [ bt-quality ] of contracted-ICT > infrastructure-quality [
      set infrastructure-quality [ bt-quality ] of contracted-ICT
      set infrastructure-cost ( [ bt-cost ] of contracted-ICT * [ bt-quality ] of contracted-ICT )
    ]

    set ISP-return-investment-time 30 * ( 1.75 - ( regulator-policy-incentivize-ISP * 0.25 ) )
  ]
  if ISP-strategy = "decrease cost" [ ;; tem 30% de chance de escolher a melhor ICT, se o preço for menor que a atual tecnologia e a qualidade não for tão ruim, implementa a tecnologia dela.
    ifelse random-in-range 0 100 < 30 [ set contracted-ICT who-min-infrastructure-price ] [ set contracted-ICT one-of ICTs ]

    if [ bt-quality ] of contracted-ICT > infrastructure-quality [ set infrastructure-quality [ bt-quality ] of contracted-ICT ]
    if ( [ bt-cost ] of contracted-ICT * [ bt-quality ] of contracted-ICT ) < infrastructure-cost and infrastructure-quality < best-ICT-tech-available * ( 0.3 + ( regulator-policy-min-quality * 0.06 ) ) [
      set infrastructure-quality [ bt-quality ] of contracted-ICT
      set infrastructure-cost ( [ bt-cost ] of contracted-ICT * [ bt-quality ] of contracted-ICT )
    ]

    set ISP-return-investment-time 20 * ( 1.75 - ( regulator-policy-incentivize-ISP * 0.25 ) )
  ]
end


to ISP-define-price
  ask ISPs [
    ifelse ISP-good-sales? = true [ ISP-change-price 5 ISP-markup ] [ ISP-change-price -5 1 ]
  ]
end

to ISP-procedure           ;; ask ISPs procedure to collect profits, define investments and set products
  calculate-ISP-congestion
  ISP-define-strategic-behavior
  ISP-investment
  if patience > 4 and ISP-access-price < 1.4 * ISP-marginal-cost [ ISP-define-price ]

  set ISP-return-investment-time ISP-return-investment-time - 1

  if ISP-access-price < ISP-marginal-cost [ set ISP-access-price ISP-marginal-cost ]
  if ISP-access-price > ( mean [ ISP-marginal-cost ] of ISPs ) * ( 1.45 - ( regulator-policy-transparency * 0.05 ) )  [ set ISP-access-price ( mean [ ISP-marginal-cost ] of ISPs ) * ( 1.45 - ( regulator-policy-transparency * 0.05 ) ) ]
  if ( ISP-utilization-price / ( ISP-access-price + ISP-utilization-price ) ) > ( regulator-policy-assymetric-regulation - 1 ) * 0.075 [ set ISP-utilization-price ( ( regulator-policy-assymetric-regulation - 1 ) * 0.075 ) * ( ISP-access-price + ISP-utilization-price ) ]

  ask my-links [ die ]

  create-link-with contracted-ICT
  ask my-links with [member? other-end ICTs] [ set color yellow ]
end

;;
;; Application Provider procedures
;;


to-report AP-average-investment-growth
  report AP-total-investment-growth / ticks
end

to-report increase-variety?
  ifelse portfolio-variety < max-portfolio-variety * min-infrastructure-quality [ report true ] [report false ]
end


to AP-investment     ;; defines AP investment
  if AP-strategic-behavior = "horizontal" [ AP-horizontal-investment ]
  if AP-strategic-behavior = "vertical" [ AP-vertical-investment ]
end


to AP-R&D
  ifelse increase-quality-is-costly? = true [
    ifelse increase-variety? = true [ set AP-strategic-behavior "horizontal" ]
    [ set AP-strategic-behavior "vertical" ]
  ] [ set AP-strategic-behavior "vertical" ]

end


to-report increase-quality-is-costly?  ;;; PRECISA DEFINIR
  ifelse portfolio-mintech-req < 0.9 * min-infrastructure-quality [
    ifelse random-in-range 0 100 < 80 [ report false ] [ report true ]
  ] [
    ifelse random-in-range 0 100 < 80 [ report true ] [ report false ]
  ]
end

to AP-horizontal-investment  ;;
  let portfolio-modifier random-normal 1.2 0.05
  ;;if random-in-range 0 100 < ( ( portfolio-variety / ( max-portfolio-variety * min-infrastructure-quality ) ) * 100 ) [
  ;;  set portfolio-modifier 1
  ;;]

  let portfolio-mintech-modifier random-normal 1.01 0.0025
  let portfolio-congestion-modifier random-normal 1.01 0.0025

  if portfolio-mintech-modifier * portfolio-mintech-req <= min-infrastructure-quality [
    set portfolio-variety ( portfolio-variety * portfolio-modifier )
    set portfolio-mintech-req ( portfolio-mintech-req * portfolio-mintech-modifier )
    set portfolio-congestion-cost ( portfolio-congestion-cost * portfolio-congestion-modifier )
  ]
end

to AP-vertical-investment
  let portfolio-modifier random-normal ( 1.10 - ( portfolio-variety / 100 ) )  0.01
  let max-quality [ portfolio-quality ] of max-one-of APs [ portfolio-quality ]
  if portfolio-modifier < 1 [ set portfolio-modifier 1 ]
  if max-quality - portfolio-quality > 5 [ set portfolio-modifier 1.15 ]

  if min-infrastructure-quality > 1 [ set portfolio-modifier portfolio-modifier * min-infrastructure-quality ]
  let portfolio-mintech-modifier random-normal 1.05 0.01
  let portfolio-congestion-modifier random-normal 1.05 0.01

  if portfolio-mintech-modifier * portfolio-mintech-req <= min-infrastructure-quality [
    set portfolio-quality ( portfolio-quality * portfolio-modifier )
    set portfolio-mintech-req ( portfolio-mintech-req * portfolio-mintech-modifier )
    set portfolio-congestion-cost ( portfolio-congestion-cost * portfolio-congestion-modifier )
  ]
end

to AP-procedure         ;; ask APs procedure to collect profits, define investments and set products
  set AP-clients count my-out-links with [ color = red ]
  if ticks mod 50 = 49 [
    if count APs > 2 [ if AP-clients = 0 [ die ] ]
  ]

  AP-R&D
  AP-investment

  if portfolio-quality > max-portfolio-quality * min-infrastructure-quality [ set portfolio-quality max-portfolio-quality * min-infrastructure-quality ]
  if portfolio-variety > max-portfolio-variety * min-infrastructure-quality [ set portfolio-variety max-portfolio-variety * min-infrastructure-quality ]
end

;;
;; ICT FIrm procedures
;;


to-report ICT-good-sales? ;;; report true if number of clients is above minimum desired
  let temp 0
  let temp-ICT self
  ask ISPs [
    if contracted-ICT = temp-ICT [ set temp temp + 1 ]
  ]
  ifelse temp >= ICT-target-clients [ report true ] [ report false ]
end

to-report ICT-average-investment-growth
  let temp 0
  ask ICTs [ set temp temp + ICT-investment-growth ]
  report temp / count ICTs
end

to-report who-min-infrastructure-price
  let min-price [ ICT-asking-price ] of min-one-of ICTs [ ICT-asking-price ]
  report one-of ICTs with [ ICT-asking-price = min-price ]
end

to-report who-best-infrastructure-quality
  let best-quality [ bt-quality ] of max-one-of ICTs [ bt-quality ]
  report one-of ICTs with [ bt-quality = best-quality ]
end

to-report best-ICT-tech-available
  let best-quality [ bt-quality ] of max-one-of ICTs [ bt-quality ]
  report best-quality
end

to-report ICT-make-R&D-investment?
  ifelse ICT-return-investment-time > 0 [ report false ] [
    report true
    set ICT-making-R&D-investment false
  ]
end

to ICT-decreasecost
  ifelse ( [ bt-quality] of who-best-infrastructure-quality ) - bt-quality > 1.5 [ ICT-newlinktech ]
  [
    let temp random-normal -0.01 0.01
    if temp < 0 [ set bt-cost bt-cost * ( 1 + temp ) ]
    set ICT-investment-growth random-normal 1.05 0.01
    set ICT-return-investment-time ( random-in-range 9 12 ) * ( 1.6 - ( regulator-policy-Incentivize-ICT * 0.2 ) )
    set ICT-making-R&D-investment true
    set innovation-counter innovation-counter + 1
  ]
end

to ICT-newlinktech
  let new-tech-quality random-normal 1.4 0.1
  let new-tech-cost random-normal 1.8 0.2
  ifelse ( bt-quality * new-tech-quality / bt-cost * new-tech-cost ) > ( bt-quality / bt-cost ) and new-tech-quality > bt-quality [ ;; If new tech quality/cost is better than old tech, change tech
    set bt-quality bt-quality * new-tech-quality
    set bt-cost bt-cost * new-tech-cost
    set ICT-investment-growth random-normal 1.12 0.02
    set ICT-return-investment-time ( random-in-range 18 24 ) * ( 1.6 - ( regulator-policy-Incentivize-ICT * 0.2 ) )
    set ICT-making-R&D-investment true
    set innovation-counter innovation-counter + 1
  ] [
    set ICT-investment-growth random-normal 1.05 0.01
    set ICT-return-investment-time ( random-in-range 9 12 ) * ( 1.6 - ( regulator-policy-Incentivize-ICT * 0.2 ) )
    set ICT-making-R&D-investment true
    set innovation-counter innovation-counter + 1
  ]
end

to ICT-investment
  ifelse ICT-make-R&D-investment? = true [
    ifelse bt-quality < best-ICT-tech-available * ( 0.3 + ( regulator-policy-min-quality * 0.04 ) ) [ ICT-newlinktech ]
    [
      ifelse bt-cost > 1 [ ICT-decreasecost ] [
        ifelse bt-cost > 0.4 [ ifelse random-in-range 1 3 != 3 [ ICT-decreasecost ] [ ICT-newlinktech ] ]
        [ ICT-newlinktech ]
      ]
    ]
  ] [ if ICT-making-R&D-investment = false [ set ICT-investment-growth random-normal 0.99 0.01 ] ]
end

to ICT-procedure         ;; ask ICTs procedure to collect profits, define investments and set products
  ICT-investment
  ICT-define-price
  ;; set ICT-total-investment-growth ICT-total-investment-growth + ICT-investment-growth
  set ICT-return-investment-time ( ICT-return-investment-time - 1 )

  if bt-quality < 0.9 [ die ]
end

to ICT-define-price
  ifelse ICT-good-sales? = true [ ICT-change-price 2.5 ] [ ICT-change-price -2.5 ]
  if ICT-asking-price < bt-cost [ set ICT-asking-price bt-cost ]
end

;;
;; Runtime Procedures
;;

to run-economy
  ask ICTs [ ICT-procedure ]
  ask IUs [ IU-procedure ]
  ask APs [ AP-procedure ]
  ask ISPS [ ISP-procedure ]

  if AP-population < 3 [ if random-in-range 0 10 < 6 [ create-APs 1 [ AP-setup-2 ] ] ]

  set innovation-index ( ( mean [ infrastructure-quality  ] of ISPs ) / ( max-ICT-tech * 0.9 ) ) * 100 ;; taxa de adoção da melhor tecnologia de ICT possível
  if innovation-index > 100 [ set innovation-index 100 ]

  set economic-efficiency-index sector-efficiency

  set universal-access-index ISP-coverage

  set avg-ICT-investment ICT-average-investment-growth
  set avg-AP-quality AP-average-quality
  set avg-AP-variety AP-average-variety

end

to-report sector-efficiency

  ifelse ( mean [ ISP-access-price ] of ISPs ) >= ( mean [ ISP-marginal-cost ] of ISPs ) and ( mean [ ISP-access-price ] of ISPs ) < 1.4 * ( mean [ ISP-marginal-cost ] of ISPs ) [
    report ( ( ( ( 1.4 * mean [ ISP-marginal-cost ] of ISPs ) / ( mean [ ISP-access-price ] of ISPs ) ) - 1 ) * 180 ) + 32
  ]
  [ report 25 ]

end

to-report EC-index
  report ( EC-economic-efficiency * economic-efficiency-index ) + ( EC-universal-access * universal-access-index ) + ( EC-innovation + innovation-index )
end

to regulator-action    ;; if EC-index < satisfatory -> change rule randomly

  if EC-index < 360 [
    ifelse EC-index < 240 [
      let temp random-in-range 0 100
      if temp <= 20 [ change-policies ]
    ] [
      let temp random-in-range 0 100
      if temp <= 5 [ change-policies ]
    ]
  ]

  regulator-incentives-check

   if ( random-normal 1 0.1 ) < 0.75 + ( 0.01 * regulator-policy-transparency ) [ ;; idealmente ess chance base (5) deveria estar relacionada há algum indicador de desempenho
